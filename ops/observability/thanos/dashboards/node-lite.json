{ "title":"Host (Node Exporter) - Starlord","panels":[
  { "type":"timeseries","title":"CPU %","targets":[{"expr":"100 - (avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)"}]},
  { "type":"timeseries","title":"Mem Used %","targets":[{"expr":"(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100"}]},
  { "type":"timeseries","title":"Disk IO (r/w)","targets":[{"expr":"rate(node_disk_reads_completed_total[5m])"},{"expr":"rate(node_disk_writes_completed_total[5m])"}]},
  { "type":"timeseries","title":"FS Used % (/)","targets":[{"expr":"(node_filesystem_size_bytes{mountpoint=\"/\"}-node_filesystem_free_bytes{mountpoint=\"/\"})/node_filesystem_size_bytes{mountpoint=\"/\"}*100"}]}
], "schemaVersion":38 }